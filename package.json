  "scripts": {
    "logos": "npm run _mkLogo192 && npm run _mkLogo512 && npm run _mkFavicon",
    "_mkLogo192": "convert -background none -resize 192x192 src/logo.svg public/logo192.png",
    "_mkLogo512": "convert -background none -resize 512x512 src/logo.svg public/logo512.png",
    "_mkFavicon": "convert public/logo512.png -define icon:auto-resize=64,48,32,16 public/favicon.ico",
    "start": "PORT=6969 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "useTabs": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.49.0",
